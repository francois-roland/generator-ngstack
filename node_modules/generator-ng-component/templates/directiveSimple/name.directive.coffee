'use strict'

(->

  ### @ngInject ###
  <%= cameledName %> = () ->
    {
      template: '<div></div>'
      restrict: 'EA'
      link : (scope, element, attrs) ->
        element.text 'this is the <% cameledName %> directive'
    }

  <%= cameledName %>
    .$inject = ['']

  angular
    .module '<%= scriptAppName %>'
    .directive '<%= cameledName %>', <%= cameledName %>


)()
