'use strict'
  
(->

  <%= cameledName %> = ($injector) ->

    # Private variables
    salutation = 'Hello'

    # Private constructor
    class Greeter
      @greet = ->
        salutation

    # Public API for configuration
    @setSalutation = (s) ->
      salutation = s

    # Method for instantiating
    @$get = ($injector) ->
      new Greeter()
      # this is the only place and method to inject dependencies in a provider
      # $q is just an exemple, you can inject anything else
      $q = $injector.get('$q')

    return

  <%= cameledName %>.$inject = ['$injector']

  angular
    .module '<%= scriptAppName %>'
    .provider '<%= cameledName %>', <%= cameledName %>


)()
